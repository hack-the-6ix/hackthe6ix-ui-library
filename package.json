{
  "name": "@hackthe6ix/vue-ui",
  "version": "0.0.1",
  "description": "",
  "sideEffects": false,
  "files": [
    "static/*",
    "dist/*"
  ],
  "repository": {
      "type": "git",
      "url": "git+https://github.com/hack-the-6ix/ui-library.git"
  },
  "author": "Hack The 6ix",
  "bugs": {
      "url": "https://github.com/hack-the-6ix/ui-library/issues"
  },
  "homepage": "https://hack-the-6ix.github.io/ui-library",
  "scripts": {
    "test": "npm run prettier -- --check",
    "lint": "npm run prettier -- --write",
    "start": "start-storybook -p 8001 -s static",
    "prepublish": "npm run lint && npm run build",
    "prettier": "prettier \"**/*.{js,jsx,vue,json,mdx}\"",
    "build": "npm run build:clean && npm run build:package",
    "build:clean": "rm -rf ./dist ./app && mkdir ./dist",
    "build:package": "cp -a ./src/. ./dist && rm -rf ./dist/**/*.stories.mdx",
    "build:storybook": "build-storybook -s static -o app"
  },
